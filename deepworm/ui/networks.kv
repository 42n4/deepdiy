<Networks>:
	BoxLayout:
		id:model_col
		Spinner:
			id:model_spinner
			text:'Select Model'
			size_hint_y:None
			height:'48dp'
	BoxLayout:
		id:weight_col
		Spinner:
			id:weight_spinner
			text:'Select Weight'
			size_hint_y:None
			height:'48dp'
			disabled:not (model_spinner.text in model_spinner.values)
	BoxLayout:
		id:run_col
		orientation: 'vertical'
		Button:
			id:btn_run
			text:'Run'
			size_hint_y:None
			height:dp(50)
			disabled: not (weight_spinner.text in weight_spinner.values)
			on_release:root.run()
