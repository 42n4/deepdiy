#:kivy 1.0

<SelectPathPanel>:
    id:select_path_panel
    Button:
        id:btn
        text: 'File'
        size_hint_y:None
        height:dp(50)
        on_press: root.open_file()
    Button:
        text: 'Folder'
        size_hint_y:None
        height:dp(50)
        on_press: root.open_folder()
