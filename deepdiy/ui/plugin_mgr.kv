<PluginManager>:
	title:'Manage Plugins'
	auto_dismiss:False
	BoxLayout:
		orientation:'vertical'
		padding:10
		BoxLayout:
			size_hint_y:None
			height:'40dp'
			Button:
				text:'Reload All Plugins'
				on_release:root.reload_all_plugins()
			Button:
				text:'Close'
				on_release:root.dismiss()
		StackLayout:
			id:plugin_album
